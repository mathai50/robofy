{
  "name": "robofy-website",
  "version": "0.1.0",
  "description": "Robofy - AI-powered digital solutions provider for business. Modern web application built with Next.js 15, React 19, TypeScript, and Tailwind CSS. Features AI integration, responsive design, and performance optimizations.",
  "keywords": [
    "ai first digital solution provider",
    "robotic process automation",
    "Intelligent competition analysis",
    "Ai lead gathering",
    "digital-marketing",
    "ai solution provider",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "private": true,
  "license": "MIT",
  "author": "Robofy Team <team@robofy.com>",
  "homepage": "https://robofy.uk",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "node --require=./set-dns.mjs node_modules/next/dist/bin/next dev -p ${PORT:-3000}",
    "dev:backend": "cd backend && uvicorn main:app --host 0.0.0.0 --reload",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "generate:content": "bash scripts/generate-content.sh",
    "dev:content": "cd backend && python3 autoblogging.py --watch",
    "test:frontend": "jest",
    "test:backend": "cd backend && python3 -m pytest",
    "test": "npm run test:frontend && npm run test:backend",
    "kill-port-3000": "npx kill-port 3000",
    "dev:alt": "PORT=3001 npm run dev:frontend"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.44",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "ai": "^5.0.44",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.16",
    "gsap": "^3.13.0",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-bits": "^1.0.5",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.0.0",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "babel-jest": "^30.0.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "nx": "21.5.2",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

