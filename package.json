{
  "name": "robofy-website",
  "version": "0.1.0",
  "description": "Robofy - AI-powered digital solutions provider for business. Modern web application built with Next.js 15, React 19, TypeScript, and Tailwind CSS. Features AI integration, responsive design, and performance optimizations.",
  "keywords": [
    "ai first digital solution provider",
    "robotic process automation",
    "Intelligent competition analysis",
    "Ai lead gathering",
    "digital-marketing",
    "ai solution provider",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "private": true,
  "license": "MIT",
  "author": "Robofy Team <team@robofy.com>",
  "homepage": "https://robofy.uk",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "node --require=./set-dns.mjs node_modules/next/dist/bin/next dev -p ${PORT:-3000}",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "generate:content": "bash scripts/generate-content.sh",
    "test:frontend": "jest",
    "test": "npm run test:frontend",
    "kill-port-3000": "npx kill-port 3000",
    "dev:alt": "PORT=3001 npm run dev:frontend"
  },
